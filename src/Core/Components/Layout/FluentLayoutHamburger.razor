@namespace Microsoft.FluentUI.AspNetCore.Components
@inherits FluentComponentBase

<div class="@ClassValue" style="@StyleValue" id="@Id"
     display="@(ShowMobileOnly ? "mobile" : "mobile-desktop")"
     @attributes="@AdditionalAttributes">
    <FluentIcon Value="@Icon" Title="@Title" Focusable="true" OnClick="@HamburgerClickAsync" />
</div>
