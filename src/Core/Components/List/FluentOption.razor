@namespace Microsoft.FluentUI.AspNetCore.Components
@inherits FluentComponentBase
@typeparam TOption
<fluent-option @ref=Element
               class="@Class"
               style="@Style"
               id="@OptionId"
               disabled="@Disabled"
               value="@Value"
               selected="@Selected"
               @onclick="@OnSelectHandler"
               @attributes="AdditionalAttributes">
    @ChildContent
</fluent-option>
