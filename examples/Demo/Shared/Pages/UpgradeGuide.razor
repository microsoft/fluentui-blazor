@page "/UpgradeGuide"

<PageTitle>@App.PageTitle("Upgrade Guide")</PageTitle>

<h1>Upgrade Guide</h1>

<MarkdownSection FromAsset="./_content/FluentUI.Demo.Shared/docs/UpgradeGuide.md" OnContentConverted="RefreshTableOfContents" />

@code {

    [CascadingParameter]
    public EventCallback OnRefreshTableOfContents { get; set; }

    private async Task RefreshTableOfContents()
    {
        await OnRefreshTableOfContents.InvokeAsync();
    }
}
