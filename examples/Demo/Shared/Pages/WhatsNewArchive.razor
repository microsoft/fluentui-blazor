@page "/WhatsNew-Archive"

<PageTitle>@App.PageTitle("What's new (archive)?")</PageTitle>

<h1>What's new?</h1>

<MarkdownSection FromAsset="./_content/FluentUI.Demo.Shared/docs/WhatsNew-Archive.md" OnContentConverted="RefreshTableOfContents" />

@code {

    [CascadingParameter]
