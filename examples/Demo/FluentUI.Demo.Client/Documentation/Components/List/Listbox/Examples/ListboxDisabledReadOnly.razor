<FluentStack Wrap="true" HorizontalGap="20px">
    <FluentListbox Label="Disabled"
                   Disabled="true"
                   Items="@Colors"
                   @bind-Value="@Value" />

    <FluentListbox Label="ReadOnly"
                   ReadOnly="true"
                   Items="@Colors"
                   @bind-Value="@Value" />
</FluentStack>

@code {
    static string[] Colors = ["Red", "Green", "Blue"];
    string? Value = "Red";
}
