@page "/DesignTokens"

<MarkdownSection FromAsset="./_content/FluentUI.Demo.Shared/docs/DesignTokens.md" OnContentConverted="ReloadToC" />

@code {

    [CascadingParameter]
    public EventCallback OnMarkdownConverted { get; set; }

    private async Task ReloadToC()
    {
        await OnMarkdownConverted.InvokeAsync();
    }
}